<tr data-position='<%= rosterSpot.player.get('position') %>'
    data-roster-spot='<%= rosterSpot.get('position') %>'
    data-id='<%= rosterSpot.player.id %>'
    data-roster-id='<%= rosterSpot.id %>'>
  <td>
    <%= rosterSpot.get('position') %>
  </td>
  <td class='actions'>
    <% if (rosterSpot.player.get('team')) { %>
      <% if (rosterSpot.player.get('team').current_team) { %>
        <button class="drop-player btn btn-danger btn-xs" id='drop-<%= rosterSpot.player.id %>'
                data-id='<%= rosterSpot.player.id %>' data-name='<%= rosterSpot.player.fullName() %>'
                data-container="body" data-placement="top">
          <span class='glyphicon glyphicon-minus-sign'></span>
        </button>
      <% } else if (player.get('team').id) { %>
        <button class="trade-player btn btn-primary btn-xs" data-id='<%= rosterSpot.player.id %>'
                data-toggle="modal" data-target="#player-action-modal">
          <span class='glyphicon glyphicon-transfer'></span>
        </button>
      <% } else { %>
        <button class="add-player btn btn-success btn-xs" data-id='<%= rosterSpot.player.id %>'
                data-toggle="modal" data-target="#player-action-modal">
          <span class='glyphicon glyphicon-plus-sign'></span>
        </button>
    <% }} %>
    <button class='watch-player btn btn-default btn-xs' data-id='<%= rosterSpot.player.id %>'>
      <span class='glyphicon glyphicon-eye-open'></span>
    </button>
  </td>
  <td>
    <%= rosterSpot.player.fullName() %><br>
    <small class='smaller'>
      <%= rosterSpot.player.get('position') %> / <%= rosterSpot.player.get('nfl_team') %>
      <% if (rosterSpot.player.get('team') && rosterSpot.player.get('team'.name)) { %>
        / <%= rosterSpot.player.get('team').name %>
      <% } %>
    </small>
  </td>
  <td>
    <%= rosterSpot.player.get('pass_yards') %>
  </td>
  <td>
    <%= rosterSpot.player.get('pass_tds') %>
  </td>
  <td>
    <%= rosterSpot.player.get('pass_ints') %>
  </td>
  <td>
    <%= rosterSpot.player.get('rush_yards') %>
  </td>
  <td>
    <%= rosterSpot.player.get('rush_tds') %>
  </td>
  <td>
    <%= rosterSpot.player.get('rec_yards') %>
  </td>
  <td>
    <%= rosterSpot.player.get('rec_tds') %>
  </td>
  <td>
    <%= rosterSpot.player.get('two_pt_conv') %>
  </td>
  <td>
    <%= rosterSpot.player.get('fumbles') %>
  </td>
  <td>
    <%= parseFloat(Math.round(rosterSpot.player.points() * 100) / 100).toFixed(2) %>
  </td>
</tr>